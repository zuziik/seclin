# sys_nice for rsyslog
# cjp: why net_admin!
allow syslogd_t self:capability { dac_override setgid setuid sys_resource sys_tty_config net_admin sys_admin sys_nice chown fsetid };
dontaudit syslogd_t self:capability sys_tty_config;
