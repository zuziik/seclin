########################################
#
# Daemon policy
#

# dac_read_search : read /run/exim/*
# net_raw         : create raw sockets
# sys_ptrace      : trace processes
allow monit_t self:capability { dac_read_search net_raw sys_ptrace };
# kernel bug
dontaudit monit_t self:capability dac_override;
# setsockopt
dontaudit monit_t self:capability net_admin;

allow monit_t self:process { getpgid sigkill signal };
allow monit_t self:fifo_file { open { getattr read write append ioctl lock } };
allow monit_t self:netlink_route_socket { { create { ioctl read getattr write setattr append bind connect getopt setopt shutdown } } nlmsg_read };
allow monit_t self:rawip_socket { create ioctl read getattr write setattr append bind getopt setopt shutdown };
allow monit_t self:sem { associate getattr read write unix_read unix_write };
allow monit_t self:tcp_socket { { create { ioctl read getattr write setattr append bind connect getopt setopt shutdown } } listen accept };
allow monit_t self:udp_socket { create { ioctl read getattr write setattr append bind connect getopt setopt shutdown } };
allow monit_t self:unix_stream_socket { { create { ioctl read getattr write setattr append bind connect getopt setopt shutdown } } listen accept };

allow monit_t monit_etc_t:dir { getattr search open read lock ioctl };
allow monit_t monit_etc_t:file { getattr open read lock ioctl };
allow monit_t monit_etc_t:lnk_file { getattr read };

allow monit_t monit_log_t:file { create { getattr open read lock ioctl } { getattr open append lock ioctl } };
 	 	
